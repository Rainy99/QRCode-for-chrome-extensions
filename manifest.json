{
    "manifest_version": 2,
    "name": "生成二维码",
    "description": "用于生成当前页面的二维码",
    "version": "1.0",
    "permissions": [
          "contextMenus",
          "tabs"
        ],
    "icons":{
        "16":"icon.png",
        "48":"icon.png",
        "128":"icon.png"
    },
    "browser_action": {
        "default_icon": "safari.png",
        "default_title": "生成二维码",
        "default_popup": "content.html"
    },
    "options_page": "options.html"
   }